{
	"include": ["index.tsx"],
	"compilerOptions": {
		"moduleResolution": "node",
		"lib": ["dom", "es5", "es6", "es2015", "es7", "es2016", "es2017", "es2018", "esnext"],
		"jsx": "react",
		"skipLibCheck": true,
		"strict": true,
		"noImplicitReturns": true,
		"noImplicitThis": true,
		"noImplicitAny": true,
		"noUnusedLocals": false,
		"noUnusedParameters": false,
		"strictNullChecks": true,
		"suppressImplicitAnyIndexErrors": true,
		"forceConsistentCasingInFileNames": false,
		"allowSyntheticDefaultImports": true,
		"resolveJsonModule": true,
		"module": "commonjs",
		"target": "es5",
		"sourceMap": false,
		"noEmit": false,
		"rootDir": ".",
		"baseUrl": ".",
		"outDir": "build",
		"allowJs": false,
		"declaration": true
	}
}
